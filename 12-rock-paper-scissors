public Integer rockPaperScissors(String player1, String player2) {
    //code here
    if(player1.equals(player2)) return 0;
    if(player1.equals('rock')){
        return player2.equals('paper')?2:1;
    }
    if(player1.equals('paper')){
        return player2.equals('scissors')?2:1;
    }
    return player2.equals('rock')?2:1;
}