public Integer fibonacci(Integer n) {
    //code here
    /*if(n==1 || n==2) return 1;
    Integer j=1;
    Integer k=1;
    Integer result;
    for(Integer i=3;i<=n;i++){
        result=j+k;
        j=k;
        k=result;
    }
    return result;*/

    //recursive implementation
    if(n==1 || n==2) return 1;
    else return fibonacci(n-1)+fibonacci(n-2);
}